{
  "name": "Modern Purple Theme Dark",
  "type": "dark",
  "colors": {
    "activityBar.background": "#0d0225fa",
    "activityBarBadge.background": "#e85c71",
    "activityBarBadge.foreground": "#0d0225fa",
    "badge.background": "#5d4b85b2",
    "badge.foreground": "#ffffff",
    "button.background": "#375dda",
    "diffEditor.insertedTextBackground": "#29793573",
    "diffEditor.removedTextBackground": "#ce838e8a",
    "editor.background": "#1b11339a",
    "editor.findMatchBackground": "#8087e4bb",
    "editor.findMatchHighlightBackground": "#42335fd5",
    "editor.foreground": "#9a9af0fa",
    "editorBracketMatch.background": "#1b11339a",
    "editorBracketMatch.border": "#e85c71",
    "editorGroupHeader.tabsBackground": "#342f42f6",
    "editorLink.activeForeground": "#e85c71",
    "gitDecoration.modifiedResourceForeground": "#a9d882b9",
    "icon.foreground": "#e85c71",
    "list.activeSelectionBackground": "#6471e423",
    "list.dropBackground": "#e85c714b",
    "list.errorForeground": "#ce838eb9",
    "list.focusOutline": "#6471e454",
    "list.hoverBackground": "#6471e423",
    "list.hoverForeground": "#ffffff",
    "list.inactiveSelectionBackground": "#64698f54",
    "list.warningForeground": "#8bbcd3d2",
    "listFilterWidget.background": "#473d3d",
    "listFilterWidget.outline": "#6471e454",
    "notificationCenter.border": "#e85c71",
    "notificationCenterHeader.background": "#e85c71",
    "notificationCenterHeader.foreground": "#1b11339a",
    "notifications.background": "#1b1133",
    "panel.background": "#0d0225fa",
    "panelSectionHeader.background": "#4c0bd8fa",
    "sideBar.background": "#0d0225fa",
    "sideBar.foreground": "#e2e2e2af",
    "sideBarTitle.foreground": "#ffffff",
    "statusBar.background": "#1b11339a",
    "statusBar.foreground": "#c7c7c7",
    "tab.activeBorder": "#ffffff",
    "tab.activeForeground": "#ffffff",
    "tab.activeModifiedBorder": "#ffffff",
    "tab.hoverBorder": "#ffffff",
    "tab.inactiveBackground": "#342f42f6",
    "tab.inactiveForeground": "#ffffff",
    "terminal.background": "#0f032b",
    "titleBar.activeBackground": "#0d0225fa",
    "titleBar.activeForeground": "#ffffff",
    "titleBar.border": "#0d0225fa",
    "titleBar.inactiveBackground": "#37304bf6",
    "titleBar.inactiveForeground": "#ffffff"
